	<queryString>
		<![CDATA[SELECT
     m_inoutline."description" AS m_inoutline_description,
     m_inoutline."movementqty" AS m_inoutline_movementqty,
     m_inoutline."m_inout_id" AS m_inoutline_m_inout_id,
     c_uom."uomsymbol" AS c_uom_uomsymbol,
     m_product."name" AS m_product_name,
     m_product."producttype" AS m_product_producttype,
     m_inoutline."m_inoutline_id" AS m_inoutline_m_inoutline_id,
     m_inoutline."line" AS m_inoutline_line,
     m_product."value" AS m_product_value,
     m_product."description" AS m_product_description,
     m_product."help" AS m_product_help,
     m_inoutline."name" AS m_inoutline_name,
     m_inoutline."help" AS m_inoutline_help,
     ad_orginfo."lit_report_param" AS ad_orginfo_lit_report_param,
     c_orderline."priceactual" AS c_orderine_priceactual,
      c_orderline."pricelist" AS c_orderine_pricelist,
        c_uom_trl."uomsymbol" AS c_uom_trl_uomsymbol,
     c_uom_trl."name" AS c_uom_trl_name,
     c_order."documentno" AS c_order_documentno,
     c_order."poreference" AS c_order_poreference

FROM
     "adempiere"."m_product" m_product RIGHT OUTER JOIN "adempiere"."m_inoutline" m_inoutline ON m_product."m_product_id" = m_inoutline."m_product_id"
     LEFT OUTER JOIN "adempiere"."c_uom" c_uom ON m_inoutline."c_uom_id" = c_uom."c_uom_id"
     RIGHT OUTER JOIN "adempiere"."ad_orginfo" ad_orginfo ON m_inoutline."ad_org_id" = ad_orginfo."ad_org_id"
      LEFT OUTER JOIN "adempiere"."c_orderline" c_orderline ON c_orderline."c_orderline_id"=m_inoutline."c_orderline_id"
      LEFT OUTER JOIN "adempiere"."c_order" c_order ON c_order."c_order_id"=c_orderline."c_order_id"
     LEFT OUTER JOIN "adempiere"."lit_intrastat" lit_intrastat ON m_product."lit_intrastat_id" = lit_intrastat."lit_intrastat_id"
   LEFT OUTER JOIN "adempiere"."c_uom_trl" c_uom_trl ON m_product."c_uom_id" = c_uom_trl."c_uom_id"
      AND c_uom_trl."ad_language" ='it_IT'
WHERE
     m_inoutline."m_inout_id" = $P{RECORD_ID}
ORDER BY
  c_order.c_order_id,   m_inoutline_line ASC]]>
